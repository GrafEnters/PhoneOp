  SayToOperator: "\u0422\u0430\u043A. \u0421 5, \u043F\u043E\u0436\u0430\u043B\u0443\u0439\u0441\u0442\u0430."
  lines:
  - "\u0422\u0430\u043A."
  - "\u0410\u0433\u0430."
  - "\u0412\u0441\u0451 \u0438\u0437-\u043F\u043E\u0434 \u0431\u0443\u0434\u043A\u0438
    \u0437\u0430\u0431\u0440\u0430\u043B. \u0423\u0434\u0438\u0432\u0438\u0442\u0435\u043B\u044C\u043D\u043E,
    \u0447\u0442\u043E \u043D\u0435 \u0441\u043F\u0430\u043B\u0438\u043B\u0438."
  - "\u0427\u0442\u043E, \u0442\u0430\u043A \u043F\u043B\u043E\u0445\u043E \u043B\u0435\u0436\u0430\u043B\u043E?"
  - "\u041E\u0447\u0435\u043D\u044C. \u0422\u044B \u0431\u044B \u0435\u0449\u0435
    \u0442\u0430\u0431\u043B\u0438\u0447\u043A\u0443 \u043F\u043E\u0432\u0435\u0441\u0438\u043B."
  - "\u0418\u0434\u0438 \u0442\u044B."
  - "\u0412\u043E\u0442 \u0438 \u043F\u043E\u0439\u0434\u0443. \u0411\u043E\u0441\u0441\u0443
    \u043E\u0442\u0434\u0430\u0432\u0430\u0442\u044C."
  - "\u0410 \u0434\u043E\u043B\u044F?"
  - "\u041E\u0442\u0441\u0447\u0438\u0442\u0430\u044E \u0442\u0435\u0431\u0435, \u043D\u0435
    \u0431\u043E\u0438\u0441\u044C."
  - "...\u0427\u0442\u043E \u0442\u0430\u043C \u043F\u043E \u0441\u043B\u0435\u0434\u0443\u044E\u0449\u0435\u043C\u0443
    \u0434\u0435\u043B\u0443?"
  - "\u0412\u0441\u0451 \u0440\u0430\u0441\u0441\u043A\u0430\u0436\u0443, \u043D\u043E
    \u043F\u043E\u0442\u043E\u043C. \u0412 \u043E\u0431\u044B\u0447\u043D\u043E\u043C
    \u043C\u0435\u0441\u0442\u0435, \u0432 \u043E\u0431\u044B\u0447\u043D\u043E\u0435
    \u0432\u0440\u0435\u043C\u044F."
  - "\u0410\u0433\u0430, \u043F\u043E\u043D\u044F\u043B."
  - "\u0418 \u043F\u043E\u0443\u0447\u0438\u0441\u044C \u0445\u0430\u0431\u0430\u0440
    \u043F\u0440\u044F\u0442\u0430\u0442\u044C. \u0421\u043A\u043E\u043B\u044C\u043A\u043E
    \u043C\u043E\u0436\u043D\u043E."
  - "\u0414\u043E \u044D\u0442\u043E\u0433\u043E \u0440\u0430\u0431\u043E\u0442\u0430\u043B\u043E..."
  - "\u0427\u0443\u0434\u0435\u0441\u0430 \u043D\u0430\u044F\u0432\u0443. \u041B\u0430\u0434\u043D\u043E,
    \u044F \u043F\u043E\u0448\u0435\u043B."
  - "\u0414\u043E \u0437\u0430\u0432\u0442\u0440\u0430."
