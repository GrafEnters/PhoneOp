  SayToOperator: "\u041E\u0439, \u0441 7 \u0441\u043E\u0435\u0434\u0438\u043D\u0438\u0442\u0435,
    \u043F\u043E\u0436\u0430\u043B\u0443\u0439\u0441\u0442\u0430."
  lines:
  - "\u041A\u0430\u0442\u0435\u0440\u0438\u043D\u0430, \u0442\u044B?"
  - "\u0410\u043B\u043B\u043E? \u041D\u0430\u0442\u0430\u0448?"
  - "\u042F, \u044F. \u0422\u044B \u044D\u0442\u043E \u0441\u043B\u044B\u0448\u0438\u0448\u044C?"
  - "\u0423\u0433\u0443\u043C. \u0423\u0436\u0435 \u043C\u0438\u043D\u0443\u0442
    10 \u0441\u0442\u0443\u0447\u0443 \u0435\u043C\u0443 \u043F\u0430\u043B\u043A\u043E\u0439
    \u0432 \u043F\u043E\u0442\u043E\u043B\u043E\u043A - \u043D\u0435 \u0441\u043B\u044B\u0448\u0438\u0442.
    \n\u0421\u043A\u043E\u043B\u044C\u043A\u043E \u043C\u043E\u0436\u043D\u043E,
    \u0437\u0430\u0432\u0442\u0440\u0430 \u043D\u0430 \u0440\u0430\u0431\u043E\u0442\u0443
    \u0440\u0430\u043D\u043E \u0432\u0441\u0442\u0430\u0432\u0430\u0442\u044C!.."
  - "\u0410 \u043A\u043E\u0433\u0434\u0430 \u043E\u043D \u043D\u0430\u0447\u0430\u043B
    \u0448\u0443\u043C\u0435\u0442\u044C? \u0427\u0430\u0441 \u043D\u0430\u0437\u0430\u0434?"
  - "\u0422\u043E\u0433\u0434\u0430 \u0435\u0449\u0435 \u043A\u0443\u0434\u0430 \u043D\u0438
    \u0448\u043B\u043E \u0431\u044B\u043B\u043E. \n\u0410 \u0441\u0435\u0439\u0447\u0430\u0441
    \u0432\u043E\u043E\u0431\u0449\u0435 \u0437\u0430 \u0433\u0440\u043E\u043C\u043A\u043E\u0441\u0442\u044C\u044E
    \u043D\u0435 \u0441\u043B\u0435\u0434\u044F\u0442."
  - "\u041C\u043E\u0436\u0435\u0442, \u0412\u0430\u0434\u0438\u043C\u0443 \u043D\u0430\u0436\u0430\u043B\u043E\u0432\u0430\u0442\u044C\u0441\u044F?
    \u041F\u0443\u0441\u0442\u044C \u043F\u043E\u0433\u043E\u0432\u043E\u0440\u044F\u0442
    \u043F\u043E-\u043C\u0443\u0436\u0441\u043A\u0438."
  - "\u0414\u0430 \u0447\u0442\u043E \u043E\u043D \u0441\u0434\u0435\u043B\u0430\u0435\u0442.
    \n\u0422\u0443\u0442 \u0443\u0436\u0435 \u0432 \u043C\u0438\u043B\u0438\u0446\u0438\u044E
    \u043D\u0430\u0434\u043E \u0437\u0430\u044F\u0432\u043B\u0435\u043D\u0438\u0435
    \u043F\u0438\u0441\u0430\u0442\u044C. \u0410 \u044D\u0442\u043E \u0442\u043E\u043B\u044C\u043A\u043E
    \u0443\u0442\u0440\u043E\u043C."
  - "\u042D\u0445. \u0412\u043E\u0442 \u0431\u044B \u043C\u043E\u0436\u043D\u043E
    \u0431\u044B\u043B\u043E \u0438\u0445 \u0441\u0435\u0439\u0447\u0430\u0441 \u0432\u044B\u0437\u0432\u0430\u0442\u044C,
    \u0430 \u043D\u0435 \u0436\u0434\u0430\u0442\u044C \u0443\u0442\u0440\u0430."
  - "\u041C\u0435\u0447\u0442\u0430\u0439 \u0434\u0430\u043B\u044C\u0448\u0435.\n\u041C-\u0434\u0430.
    \u0411\u0435\u0440\u0443\u0448\u0438-\u0442\u043E \u0445\u043E\u0442\u044C \u0443
    \u0442\u0435\u0431\u044F \u0435\u0441\u0442\u044C? \u041C\u043E\u0433\u0443 \u043F\u043E\u0434\u0435\u043B\u0438\u0442\u044C\u0441\u044F."
  - "\u0415\u0441\u0442\u044C-\u0435\u0441\u0442\u044C. \u041F\u043E\u043F\u0440\u043E\u0431\u0443\u0435\u043C
    \u043F\u0435\u0440\u0435\u0442\u0435\u0440\u043F\u0435\u0442\u044C, \u0432\u0438\u0434\u0438\u043C\u043E.\n\u0414\u043E\u0431\u0440\u043E\u0439
    \u043D\u043E\u0447\u0438."
  - "\u0425\u0430\u0445. \u041D\u043E\u0447\u0438."
