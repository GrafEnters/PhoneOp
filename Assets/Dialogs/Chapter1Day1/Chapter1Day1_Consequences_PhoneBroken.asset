  SayToOperator: "\u0423\u0445. \u041D\u0430\u0434\u0435\u044E\u0441\u044C, \u043D\u0435
    \u0437\u0430\u0441\u0442\u0443\u043A\u0430\u044E\u0442.\n\u0421 9 \u043C\u043E\u0436\u043D\u043E?"
  lines:
  - "\u0422\u0438\u043C?"
  - "\u0422\u0430\u0441\u044F? \u0422\u044B \u0447\u0435\u0433\u043E, \u0441 \u0440\u0430\u0431\u043E\u0442\u044B
    \u0437\u0432\u043E\u043D\u0438\u0448\u044C?"
  - "\u0414\u0430 \u044F \u0431\u044B\u0441\u0442\u0440\u043E. \u0422\u044B \u043D\u043E\u0432\u0443\u044E
    \u043F\u0440\u0438\u0441\u0442\u0440\u043E\u0439\u043A\u0443 \u0432\u0438\u0434\u0435\u043B?"
  - "\u0421 \u044D\u0442\u0438\u043C, \u043A\u0430\u043A \u0435\u0433\u043E, \u0442\u0430\u043A\u0441\u043E\u0444\u043E\u043D\u043E\u043C?
    \u0415\u0449\u0435 \u0432\u0447\u0435\u0440\u0430 \u0432\u0438\u0434\u0435\u043B."
  - "\u041D\u0443, \u044F \u0441\u0435\u0433\u043E\u0434\u043D\u044F \u0441\u043F\u0435\u0446\u0438\u0430\u043B\u044C\u043D\u043E
    \u0432\u0441\u0442\u0430\u043B\u0430 \u043F\u043E\u0440\u0430\u043D\u044C\u0448\u0435,
    \n\u0447\u0442\u043E\u0431\u044B \u0443\u0441\u043F\u0435\u0442\u044C \u043F\u043E\u0441\u043C\u043E\u0442\u0440\u0435\u0442\u044C.
    \u0418 \u043F\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043B\u044F\u0435\u0448\u044C:
    \n\u043E\u043D\u0430 \u0441\u043E\u0432\u0441\u0435\u043C \u0441\u043B\u043E\u043C\u0430\u043D\u043D\u0430\u044F
    \u0441\u0442\u043E\u0438\u0442! \u0413\u043E\u0432\u043E\u0440\u044F\u0442, \u0434\u043D\u044F
    \u0434\u0432\u0430 \u0447\u0438\u043D\u0438\u0442\u044C \u0431\u0443\u0434\u0443\u0442."
  - "\u0421\u043B\u043E\u043C\u0430\u043D\u043D\u0430\u044F? "
  - "\u0410\u0433\u0430. \u0410\u043B\u044F \u0433\u043E\u0432\u043E\u0440\u0438\u0442,
    \u0427\u0435\u0440\u043D\u0430\u044F \u0440\u0443\u043A\u0430 \u0432\u0447\u0435\u0440\u0430
    \u043F\u043E\u0441\u0442\u0430\u0440\u0430\u043B\u0430\u0441\u044C. \n\u0418
    \u043D\u0430\u0434 \u043E\u0434\u0438\u043D \u0438\u0437 \u043E\u043F\u0435\u0440\u0430\u0442\u043E\u0440\u043E\u0432
    \u0442\u043E\u0436\u0435. \n\u0412\u043E\u043D \u0432 \u0441\u0435\u0434\u044C\u043C\u043E\u0439
    \u043F\u0430\u043B\u0430\u0442\u0435 \u043B\u0435\u0436\u0438\u0442."
  - "\u0422\u043E\u043B\u044C\u043A\u043E \u043D\u0435 \u0434\u043E\u043A\u0443\u0447\u0430\u0439
    \u0435\u043C\u0443, \u0430 \u0442\u043E \u044F \u0442\u0435\u0431\u044F \u0437\u043D\u0430\u044E.
    \n\u041F\u0443\u0441\u0442\u044C \u043F\u043E\u043F\u0440\u0430\u0432\u043B\u044F\u0435\u0442\u0441\u044F
    \u0447\u0435\u043B\u043E\u0432\u0435\u043A."
  - "\u0425\u043E\u0440\u043E\u0448\u043E-\u0445\u043E\u0440\u043E\u0448\u043E! \u041D\u0435
    \u0431\u0443\u0434\u0443."
  - "\u0421\u043C\u043E\u0442\u0440\u0438 \u043C\u043D\u0435. \u0412\u0441\u0435,
    \u0431\u0435\u0433\u0438 \u043D\u0430 \u0440\u0430\u0431\u043E\u0442\u0443, \n\u043F\u043E\u043A\u0430
    \u0442\u0435\u0431\u044F \u0412\u0430\u043B\u0435\u043D\u0442\u0438\u043D\u043E\u0432\u043D\u0430
    \u043D\u0435 \u0437\u0430\u043C\u0435\u0442\u0438\u043B\u0430."
  - "\u0411\u0435\u0433\u0443-\u0431\u0435\u0433\u0443. \u0425\u043E\u0440\u043E\u0448\u0435\u0433\u043E
    \u0442\u0435\u0431\u044F \u0434\u043D\u044F \u0442\u043E\u0436\u0435."
  - "\u0421\u043F\u0430\u0441\u0438\u0431\u043E."
