  SayToOperator: "\u0423\u0445. \u041D\u0430\u0434\u0435\u044E\u0441\u044C, \u043D\u0435
    \u0437\u0430\u0441\u0442\u0443\u043A\u0430\u044E\u0442.\n-\u0421 9 \u043C\u043E\u0436\u043D\u043E?"
  lines:
  - "\u0422\u0438\u043C?"
  - "\u0422\u0430\u0441\u044F? \u0422\u044B \u0447\u0435\u0433\u043E, \u0441 \u0440\u0430\u0431\u043E\u0442\u044B
    \u0437\u0432\u043E\u043D\u0438\u0448\u044C?"
  - "\u0414\u0430 \u044F \u0431\u044B\u0441\u0442\u0440\u043E. \u0422\u044B \u043D\u043E\u0432\u0443\u044E
    \u043F\u0440\u0438\u0441\u0442\u0440\u043E\u0439\u043A\u0443 \u0432\u0438\u0434\u0435\u043B?"
  - "\u0421 \u044D\u0442\u0438\u043C, \u043A\u0430\u043A \u0435\u0433\u043E, \u0442\u0430\u043A\u0441\u043E\u0444\u043E\u043D\u043E\u043C?
    \u0415\u0449\u0435 \u0432\u0447\u0435\u0440\u0430 \u0432\u0438\u0434\u0435\u043B."
  - "\u041D\u0443, \u044F \u0441\u0435\u0433\u043E\u0434\u043D\u044F \u0441\u043F\u0435\u0446\u0438\u0430\u043B\u044C\u043D\u043E
    \u0432\u0441\u0442\u0430\u043B\u0430 \u043F\u043E\u0440\u0430\u043D\u044C\u0448\u0435,
    \u0447\u0442\u043E\u0431\u044B \u0443\u0441\u043F\u0435\u0442\u044C \u043F\u043E\u0441\u043C\u043E\u0442\u0440\u0435\u0442\u044C.\n\u042F
    \u0432\u0435\u0434\u044C \u043D\u0435 \u0432\u0438\u0434\u0435\u043B\u0430 \u0435\u0449\u0435."
  - "\u0418 \u043A\u0430\u043A \u0442\u0435\u0431\u0435?"
  - "\u0421\u0442\u0440\u0430\u0448\u043D\u0435\u043D\u044C\u043A\u043E. \u041F\u0440\u043E\u0432\u043E\u0434\u043E\u0432
    - \u0436\u0443\u0442\u044C. \u041D\u0430 \u0431\u043E\u043B\u044C\u043D\u0438\u0447\u043D\u043E\u043C
    \u0438\u0445 \u043F\u043E\u043C\u0435\u043D\u044C\u0448\u0435 \u0445\u043E\u0442\u044F
    \u0431\u044B."
  - "\u0410 \u043C\u043D\u0435 \u043A\u0430\u0436\u0435\u0442\u0441\u044F, \u0447\u0442\u043E
    \u0441\u0438\u043C\u043F\u0430\u0442\u0438\u0447\u043D\u043E. \u0417\u0430\u0440\u0443\u0431\u0435\u0436\u043D\u044B\u0435
    \u0445\u0443\u0436\u0435 \u0432\u044B\u0433\u043B\u044F\u0434\u044F\u0442."
  - "\u041D\u0443, \u0438\u043C\u043F\u0435\u0440\u0430\u0442\u043E\u0440 \u0440\u0430\u0441\u0441\u0442\u0430\u0440\u0430\u043B\u0441\u044F.
    \u0412\u0441\u0435\u0433\u043E \u043F\u0430\u0440\u0443 \u043B\u0435\u0442 \u043D\u0430
    \u0442\u0440\u043E\u043D\u0435 - \u0438 \u0432\u043E\u043D \u0441\u043A\u043E\u043B\u044C\u043A\u043E
    \u0443 \u0441\u043E\u0441\u0435\u0434\u0435\u0439 \u043F\u043E\u043D\u0430\u0432\u0451\u0437.\n\u041D\u0438\u0447\u0435\u0433\u043E
    \u0441\u0432\u043E\u0435\u0433\u043E \u0443\u0436\u0435 \u043F\u043E\u0447\u0442\u0438
    \u043D\u0435 \u043E\u0441\u0442\u0430\u043B\u043E\u0441\u044C."
  - "\u0418 \u0445\u043E\u0440\u043E\u0448\u043E. \u0421\u0442\u0430\u0440\u044B\u0439
    \u0438\u043C\u043F\u0435\u0440\u0430\u0442\u043E\u0440, \u043C\u043D\u0435 \u043A\u0430\u0436\u0435\u0442\u0441\u044F,
    \u043D\u0430\u0441 \u0442\u043E\u043B\u044C\u043A\u043E \u0437\u0430\u043A\u0440\u044B\u0442\u044C
    \u043E\u0442 \u0434\u0440\u0443\u0433\u0438\u0445 \u0445\u043E\u0442\u0435\u043B."
  - "(\u0432\u0437\u0434\u043E\u0445) \u041C\u044B \u0436\u0435 \u044D\u0442\u043E
    \u0443\u0436\u0435 \u043E\u0431\u0441\u0443\u0436\u0434\u0430\u043B\u0438..."
  - "\u0421\u043E\u043B\u043D\u0446, \u043D\u0443 \u043D\u0435\u0443\u0436\u0435\u043B\u0438
    \u0442\u044B \u043D\u0435 \u0432\u0438\u0434\u0438\u0448\u044C..."
  - "\u041C\u0435\u043D\u044F \u0442\u0430\u043C \u0437\u043E\u0432\u0443\u0442,
    \u043A\u0430\u0436\u0435\u0442\u0441\u044F. "
  - "\u041E\u0445. \u041D\u0443 \u043B\u0430\u0434\u043D\u043E, \u0431\u0435\u0433\u0438
    \u0442\u043E\u0433\u0434\u0430."
